<div class="weui-cell">
  <div class="weui-cell__bd">
    <span><%= model.identity %></span>
    <% if model.confirmed %>
      <i class="fas fa-check-circle has-text-success"></i>
    <% else %>
      <i class="fas fa-question-circle has-text-warning"></i>
    <% end %>
  </div>
  <div class="weui-cell__ft">
  
  </div>
</div>
