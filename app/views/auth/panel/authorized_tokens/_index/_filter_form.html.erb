<%= form_with theme: 'search', model: Auth::AuthorizedToken.new do |f| %>
  <div class="field-body">
    <%= f.text_field :identity %>
    <%= f.text_field :uid %>
    <%= f.text_field 'id-like' %>
    <div class="field is-narrow">
      <%= f.submit %>
      <%= link_to t('.clear'), filter_params(except: [:identity, :uid, 'token-like']), class: 'button is-light' %>
    </div>
  </div>
<% end %>
