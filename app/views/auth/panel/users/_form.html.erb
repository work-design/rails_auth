<%= form_with model: [:panel, @user] do |f| %>
  <%= f.text_field :name, required: true %>
  <%= f.fields :accounts do |ef| %>
    <%= ef.text_field :identity %>
  <% end %>
  <%= render 'com/attachments/list_field', target: @user.avatar %>
  <%= f.text_field :password %>
  <%= f.check_box :disabled %>
  <%= f.submit %>
<% end %>
