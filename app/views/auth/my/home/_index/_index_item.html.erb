<%= link_to({ controller: 'trade/my/wallet_templates' }, state: 'enter', class: 'weui-cell weui-cell_access') do %>
  <div class="weui-cell__bd">
    <i class="fa-solid fa-fw fa-wallet has-text-primary"></i>
    <span class="pl-1"><%= t('trade.my.wallet_templates.index.title') %></span>
  </div>
  <div class="weui-cell__ft"></div>
<% end %>

<% if request.variant.include?(:mini_program) %>
<div
  data-controller="mini-program"
  data-mini-program-nav-value="true"
  data-mini-program-url-value="/pages/print/config/index"
  data-mini-program-data-value="<%= { url: url_for(only_path: false) }.to_json %>"
  data-action="click->mini-program#link"
  class="weui-cell weui-cell_access"
>
  <div class="weui-cell__bd">
    <i class="fa-solid fa-fw fa-print has-text-primary"></i>
    <span class="pl-1"><%= '设置打印机' %></span>
  </div>
  <div class="weui-cell__ft"></div>
</div>
<% end %>
