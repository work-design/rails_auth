<%= link_to({ controller: 'board/accounts' }, state: 'enter', class: 'weui-cell weui-cell_access') do %>
  <div class="weui-cell__bd">
    <i class="fa-solid fa-fw fa-users has-text-primary"></i>
    <span class="pl-1"><%= t('auth.board.accounts.index.title') %></span>
  </div>
  <div class="weui-cell__ft"></div>
<% end %>
<%= link_to({ controller: 'board/oauth_users' }, state: 'enter', class: 'weui-cell weui-cell_access') do %>
  <div class="weui-cell__bd">
    <i class="fa-brands fa-fw fa-weixin has-text-primary"></i>
    <span class="pl-1"><%= t('auth.board.oauth_users.index.title') %></span>
  </div>
  <div class="weui-cell__ft"></div>
<% end %>

<%= link_to({ controller: 'profiled/my/profiles', action: 'qrcode' }, state: 'enter', class: 'weui-cell weui-cell_access') do %>
  <div class="weui-cell__bd">
    <i class="fa-solid fa-fw fa-qrcode has-text-primary"></i>
    <span class="pl-1"><%= t('profiled.my.profiles.qrcode.title') %></span>
  </div>
  <div class="weui-cell__ft"></div>
<% end %>

<%= link_to({ controller: 'profiled/my/profiles', action: 'show' }, state: 'enter', class: 'weui-cell weui-cell_access') do %>
  <div class="weui-cell__bd">
    <i class="fa-solid fa-fw fa-user has-text-primary"></i>
    <span class="pl-1"><%= t('profiled.my.profiles.show.title') %></span>
  </div>
